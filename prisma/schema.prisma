generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model User {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  passwordHash String
  name      String?
  createdAt DateTime @default(now())
  isDeleted Boolean  @default(false)
} 
